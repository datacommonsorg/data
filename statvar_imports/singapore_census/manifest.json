{
    "import_specifications": [
        {
            "import_name": "Singapore_Census",
            "curator_emails": [
                "support@datacommons.org"
            ],
            "provenance_url": "https://tablebuilder.singstat.gov.sg/",
            "provenance_description": "Population Census and Health Statistics for Singapore based on gender, age and ethnicity.",
            "scripts": [
                "download_script.py",
                "../../tools/statvar_importer/stat_var_processor.py  --input_data=input_files/DeathByCause.csv --pv_map=configs/deathbycause_pvmap.csv --config_file=configs/common_metadata.csv --output_path=output_path/DeathByCause --existing_statvar_mcf=gs://unresolved_mcf/scripts/statvar/stat_vars.mcf",
                "../../tools/statvar_importer/stat_var_processor.py  --input_data=input_files/Death_DeathRate.csv --pv_map=configs/death_deathrates_pvmap.csv --config_file=configs/common_metadata.csv --output_path=output_path/Death_DeathRate --existing_statvar_mcf=gs://unresolved_mcf/scripts/statvar/stat_vars.mcf",
                "../../tools/statvar_importer/stat_var_processor.py  --input_data=input_files/Life_Expectancy.csv --pv_map=configs/life_expectancy_by_sex_pvmap.csv --config_file=configs/common_metadata.csv --output_path=output_path/LifeExpectancy --existing_statvar_mcf=gs://unresolved_mcf/scripts/statvar/stat_vars.mcf",
                "../../tools/statvar_importer/stat_var_processor.py  --input_data=input_files/Mortality.csv --pv_map=configs/mortality_by_ethnicty_sex_pvmap.csv --config_file=configs/common_metadata.csv --output_path=output_path/Mortality --existing_statvar_mcf=gs://unresolved_mcf/scripts/statvar/stat_vars.mcf",
                "../../tools/statvar_importer/stat_var_processor.py  --input_data=input_files/Population_Indicator.csv --pv_map=configs/Population_Indicator_pvmap.csv --config_file=configs/common_metadata.csv --output_path=output_path/Population_Indicator --existing_statvar_mcf=gs://unresolved_mcf/scripts/statvar/stat_vars.mcf",
                "../../tools/statvar_importer/stat_var_processor.py  --input_data=input_files/Population_Resident.csv --pv_map=configs/Population_Resident_age_pvmap.csv --config_file=configs/common_metadata.csv --output_path=output_path/Pop_Resident --existing_statvar_mcf=gs://unresolved_mcf/scripts/statvar/stat_vars.mcf",
                "../../tools/statvar_importer/stat_var_processor.py  --input_data=input_files/Population_Citizens.csv --pv_map=configs/Population_Citizen_age_pvmap.csv --config_file=configs/common_metadata.csv --output_path=output_path/Pop_Citizen --existing_statvar_mcf=gs://unresolved_mcf/scripts/statvar/stat_vars.mcf"
            ],
            "import_inputs": [
                {
                    "template_mcf": "common_tmcf.tmcf",
                    "cleaned_csv": "output_path/*.csv"
                }
            ],
            "source_files": [
                "input_files/*"
            ],
            "resource_limits": {
                "cpu": 8,
                "memory": 32,
                "disk": 100
            },
            "cron_schedule": "30 05 14,28 * *"
        }
    ]
}