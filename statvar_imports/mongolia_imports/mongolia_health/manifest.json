{
  "import_specifications": [
    {
      "import_name": "Mongolia_Health",
      "curator_emails": ["support@datacommons.org"],
      "provenance_url": "https://www.1212.mn/en/statcate",
      "provenance_description": "Statistical data related to births and deaths in Mongolia",
      "scripts": ["../common_download_script.py", "run.sh"],
      "source_files": [
        "input_files/*.csv"
      ],
      "import_inputs": [
        {
          "template_mcf": "output_files/deaths_by_month_and_region_output.tmcf",
          "cleaned_csv": "output_files/deaths_by_month_and_region_output.csv"
        },
        {
          "template_mcf": "output_files/infant_mortality_per_1000_live_births_by_month_region_output.tmcf",
          "cleaned_csv": "output_files/infant_mortality_per_1000_live_births_by_month_region_output.csv"
        },
        {
          "template_mcf": "output_files/live_births_by_month_region_output.tmcf",
          "cleaned_csv": "output_files/live_births_by_month_region_output.csv"
        },
        {
          "template_mcf": "output_files/number_of_abortions_by_region_output.tmcf",
          "cleaned_csv": "output_files/number_of_abortions_by_region_output.csv"
        },
        {
          "template_mcf": "output_files/number_of_hospital_beds_by_type_output.tmcf",
          "cleaned_csv": "output_files/number_of_hospital_beds_by_type_output.csv"
        },
        {
          "template_mcf": "output_files/number_of_mothers_delivered_child_by_month_region_output.tmcf",
          "cleaned_csv": "output_files/number_of_mothers_delivered_child_by_month_region_output.csv"
        }
      ],
      "cron_schedule": "0 03 * * 6"
    }
  ]
}

